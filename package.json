{
  "name": "tilesy-server",
  "version": "1.0.0",
  "description": "Server for Tilesy",
  "main": "dist/index.js",
  "author": "Husnain Javed <me@husnainjaved.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node bin/dev",
    "clean": "if exist dist rmdir /q /s dist",
    "build": "babel src -d dist",
    "rebuild": "npm run clean && npm run build",
    "serve": "npm run rebuild && npm run start",
    "start": "node bin/www",
    "deploy": "npm run rebuild && npx serverless deploy --stage=staging --env staging",
    "publish": "npm run rebuild && npx serverless deploy --stage=prod --env production"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "aws-sdk": "^2.799.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "core-js": "3",
    "cors": "^2.8.5",
    "currency-symbol-map": "^4.0.4",
    "dotenv-flow": "^3.2.0",
    "dynamoose": "^2.3.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "regenerator-runtime": "^0.13.7",
    "serverless": "^2.48.0",
    "serverless-http": "^2.6.0",
    "shortid": "^2.2.15",
    "spotify-web-api-node": "^5.0.0",
    "stripe": "^8.101.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.9.0",
    "nodemon": "^2.0.4",
    "serverless-dotenv-plugin": "^3.0.0"
  }
}
